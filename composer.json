        "zf3", 
        "authentication", 
        "doctrine", 
        "ORM", 
        "freedom"
    ],
    "autoload": {
        "psr-4": {
            "FwsDoctrineAuth\\": "src/"
        }
    },
    "license": "BSD-3-Clause",
    "type": "module",
    "homepage": "https://github.com/krytenuk/fws-doctrine-auth",
    "extra": {
        "zf": {
            "module": "FwsDoctrineAuth"
        }
    }
