    "description": "Zend Framework 3 module for authentication using Doctrine",
    "require": {
        "php": "^7.0",
        "symfony/console": "^4.3",
        "doctrine/doctrine-orm-module": "^2.0",
        "container-interop/container-interop": "^1.2",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-view": "^2.10",
        "laminas/laminas-mvc-i18n": "^1.1",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-form": "^2.12",
        "laminas/laminas-inputfilter": "^2.8",
        "laminas/laminas-validator": "^2.10",
        "laminas/laminas-eventmanager": "^3.2",
        "laminas/laminas-authentication": "^2.6",
        "laminas/laminas-permissions-acl": "^2.7",
        "laminas/laminas-crypt": "^3.3",
        "laminas/laminas-session": "^2.8",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-modulemanager": "^2.8",
        "krytenuk/refresh-redirect": "^2.0",
        "laminas/laminas-navigation": "^2.9",
        "laminas/laminas-router": "^3.3",
        "laminas/laminas-math": "^3.2"
